    }
  },
  {
    "type": "ListItem",
    "element_id": "d6d2eb569828ee0ca6732f5f1195a9fb",
    "text": "Loss of appetite",
    "metadata": {
      "category_depth": 2,
      "last_modified": "2024-07-05T20:28:53",
      "languages": [
        "eng"
      ],
      "parent_id": "f405f00de812f161764d03a7b084d43b",
      "file_directory": ".",
